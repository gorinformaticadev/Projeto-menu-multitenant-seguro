# ============================================
# CONFIGURAÇÕES DE STAGING - FRONTEND
# ============================================

# Ambiente
NODE_ENV=staging

# API Backend (staging)
NEXT_PUBLIC_API_URL="http://localhost:4001"

# Configurações de build
NEXT_TELEMETRY_DISABLED=1

# Sentry (staging)
NEXT_PUBLIC_SENTRY_DSN="https://your-staging-sentry-dsn@sentry.io/project-id"
SENTRY_AUTH_TOKEN="your-sentry-auth-token"
SENTRY_ORG="your-org"
SENTRY_PROJECT="frontend-staging"

# Chave de criptografia (staging)
NEXT_PUBLIC_ENCRYPTION_KEY="staging-encryption-key-2024"

# Configurações de funcionalidades
NEXT_PUBLIC_ENABLE_ANALYTICS="false"
NEXT_PUBLIC_ENABLE_DEBUG="true"
NEXT_PUBLIC_ENABLE_DEVTOOLS="true"

# URLs de staging
NEXT_PUBLIC_APP_URL="http://localhost:5001"